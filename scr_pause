switch (mpos)
{
    case 0:
    {   
        instance_activate_all ();
        global.pause = 0;    
        break;
    }
    
    case 1:
    {
        room_goto (rm_menu);
        //to mi je ujebalo pozadinu naknadno. ako ne bude funkcioniralo, zamijeni s game_end();
        break;
    }
    default: break;
}
